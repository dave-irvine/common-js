{
  "name": "configcat-common",
  "version": "4.4.4",
  "description": "ConfigCat is a configuration as a service that lets you manage your features and configurations without actually deploying new code.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/esm/index.js",
  "scripts": {
    "coverage": "nyc npm run test",
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && tsc -p tsconfig.legacy.json && gulp",
    "prepare": "npm run build",
    "test": "mocha --require ts-node/register test/**/*.ts --exit --timeout 30000",
    "test-debug": "mocha --fgrep \"TITLE_OF_TEST\" --require ts-node/register test/**/*.ts --exit --timeout 30000"
  },
  "keywords": [
    "configcat",
    "config",
    "configuration",
    "remote configuration",
    "configcat client",
    "feature flags",
    "feature toggle",
    "feature switch",
    "canary release",
    "soft launch",
    "release strategy"
  ],
  "author": "ConfigCat",
  "license": "MIT",
  "homepage": "https://configcat.com",
  "devDependencies": {
    "@types/chai": "4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.12",
    "chai": "^4.3.4",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.1.3",
    "gulp-typescript": "^5.0.1",
    "mocha": "^9.1.1",
    "moq.ts": "^7.3.4",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/configcat/common-js"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "json",
      "lcov"
    ],
    "all": true,
    "include": [
      "src"
    ],
    "exclude": [
      "src/Semver.ts",
      "src/Sha1.ts"
    ]
  },
  "sideEffects": false
}
